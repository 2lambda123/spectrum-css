{
	"npmScope": "@spectrum-css",
	"tasksRunnerOptions": {
		"default": {
			"runner": "nx/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build"]
			}
		}
	},
	"targetDefaults": {
		"clean": {
			"inputs": ["{workspaceRoot}/dist"],
			"executor": "nx:run-commands",
			"options": {
				"commands": ["rimraf {projectRoot}/dist"]
			}
		},
		"build": {
			"dependsOn": ["clean", "^build"],
			"inputs": ["{workspaceRoot}/*.css"]
		}
	},
	"includedScripts": ["build"]
}
